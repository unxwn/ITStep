{
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ],
  "ConnectionStrings": {
    //"AzureStorageAccount": "",
    //"AzureWebJobsStorage": "",
    //"ApplicationInsights": ""
  },
  "BlobSettings": {
    "InputContainerName": "original-files",
    "OutputContainerName": "compressed-images"
  },
  "QueueTrigger": {
    "QueueName": "images-processing-queue"
  },
  "ImageSettings": {
    "Width": 700,
    "Height": 400
  },
  "Logging": {
    "ApplicationInsights": {
      "ExcludeScopes": false,
      "LogLevel": {
        "Default": "Information"
      }
    }
  }
}